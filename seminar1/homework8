// Напишите программу, которая на вход принимает число (N), а на выходе показывает все чётные числа от 1 до N.

Console.Write("Enter number: ");
string strNum = Console.ReadLine();
int num = int.Parse(strNum);

int num0 = 2;

while (num >= num0) 
{
    Console.Write (num0 + ", ");
    num0 = num0 + 2;
}
